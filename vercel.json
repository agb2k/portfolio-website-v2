{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/resume",
      "destination": "/Abhinav_George_Basil_Resume.pdf",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(pdf|jpg|jpeg|png|gif|svg|ico|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
