{
  "name": "abhinav-portfolio",
  "version": "1.0.0",
  "description": "Professional portfolio website built with Next.js 15, TypeScript, and Tailwind CSS",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true npm run build",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "portfolio",
    "nextjs",
    "typescript",
    "tailwind",
    "react",
    "vercel"
  ],
  "author": "Abhinav George Basil",
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.0.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "next": "15.0.7",
    "next-themes": "^0.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.6",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
